<tr>
  <td>
    <% if current_user.admin? %>
      <%= link_to user.family, user %>&nbsp;<%= user.given %>
    <% else %>
      <%= user.family %>&nbsp;<%= user.given %>
    <% end %>
  </td>
  <td>
    <%= user.maiden %>
  </td>
  <td>
    <%= user.notes %>
  </td>
  <td>
    <% if user.region != nil %>
      あり
    <% end %>
  </td>
  <td>
    <% if user.tel != nil %>
      あり
    <% end %>
  </td>
  <td>
    <% if user.mobile != nil %>
      あり
    <% end %>
  </td>
  <td>
    <% if user.email != nil %>
      あり
    <% end %>
  </td>
</tr>
